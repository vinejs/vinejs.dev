{
  "name": "adonisjs-web-stater-kit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "export": "vite build && npm run download:sponsors && node --loader=ts-node/esm bin/build.ts",
    "build": "npm run export",
    "postexport": "copyfiles -u 1 public/* public/**/* dist",
    "download:sponsors": "node --loader=ts-node/esm bin/download_sponsors.ts",
    "start": "node bin/test.js",
    "serve": "node --loader=ts-node/esm bin/serve.ts",
    "dev": "concurrently \"vite\" \"npm run serve\"",
    "test": "node ace test"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.1.2",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/tsconfig": "^1.4.1",
    "@alpinejs/persist": "^3.15.0",
    "@docsearch/css": "^4.2.0",
    "@docsearch/js": "^4.2.0",
    "@swc/core": "^1.13.5",
    "@types/node": "^24.7.2",
    "alpinejs": "^3.15.0",
    "collect.js": "^4.36.1",
    "concurrently": "^9.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.37.0",
    "highcharts": "^12.4.0",
    "human-format": "^1.2.1",
    "medium-zoom": "^1.1.0",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "undici": "^7.16.0",
    "unpoly": "^3.12.0",
    "vite": "^5.2.6"
  },
  "dependencies": {
    "@adonisjs/core": "^6.19.0",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^2.0.2",
    "@dimerapp/content": "^5.2.1",
    "@dimerapp/docs-theme": "^6.1.3",
    "@dimerapp/edge": "^5.0.0",
    "@radix-ui/colors": "^3.0.0",
    "edge-uikit": "^1.0.0-1",
    "edge.js": "^6.4.0"
  },
  "license": "UNLICENSED",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config"
}
