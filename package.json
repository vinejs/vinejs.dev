{
  "name": "adonisjs-web-stater-kit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "export": "vite build && npm run download:sponsors && node --loader=ts-node/esm bin/build.ts",
    "build": "npm run export",
    "postexport": "copyfiles -u 1 public/* public/**/* dist",
    "download:sponsors": "node --loader=ts-node/esm bin/download_sponsors.ts",
    "start": "node bin/test.js",
    "serve": "node --loader=ts-node/esm bin/serve.ts",
    "dev": "concurrently \"vite\" \"npm run serve\"",
    "test": "node ace test"
  },
  "imports": {
    "#src/*": "./src/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.2.3",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@alpinejs/persist": "^3.13.7",
    "@docsearch/css": "^3.6.0",
    "@docsearch/js": "^3.6.0",
    "@swc/core": "^1.4.11",
    "@types/node": "^20.11.30",
    "alpinejs": "^3.13.7",
    "collect.js": "^4.36.1",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "highcharts": "^11.4.0",
    "human-format": "^1.2.0",
    "medium-zoom": "^1.1.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "undici": "^6.10.2",
    "unpoly": "^3.7.3",
    "vite": "^5.2.6"
  },
  "dependencies": {
    "@adonisjs/core": "^6.3.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^3.0.0",
    "@dimerapp/content": "^5.0.0",
    "@dimerapp/docs-theme": "^5.0.0",
    "@dimerapp/edge": "^5.0.0",
    "@radix-ui/colors": "^3.0.0",
    "edge-uikit": "^1.0.0-1",
    "edge.js": "^6.0.2"
  },
  "prettier": "@adonisjs/prettier-config",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  }
}
